.container
  .wrap.tabs
    %p.header_text Boards
    .border_room
    %h1.tabs_text All Boards
    .tabs_box
      .task_card
        - @boards.each do |board|
          = board.title
            - link_to 'board_path'
      .add_board
        - render 'board#new' do
        .add_text +Add new board

